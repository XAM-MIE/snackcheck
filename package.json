{
  "name": "snackcheck",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "vitest",
    "test:run": "vitest run",
    "test:performance": "vitest run src/test/performance.test.ts",
    "test:comprehensive": "vitest run src/test/e2e src/test/cross-browser src/test/performance src/test/ocr src/test/health-scoring",
    "test:e2e": "vitest run src/test/e2e",
    "test:cross-browser": "vitest run src/test/cross-browser",
    "test:benchmarks": "vitest run src/test/performance",
    "test:ocr": "vitest run src/test/ocr",
    "test:health-scoring": "vitest run src/test/health-scoring",
    "test:coverage": "vitest run --coverage",
    "analyze": "ANALYZE=true npm run build",
    "build:production": "NODE_ENV=production npm run build"
  },
  "dependencies": {
    "lucide-react": "^0.540.0",
    "next": "15.4.7",
    "next-pwa": "^5.6.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "tesseract.js": "^6.0.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.7.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@vitejs/plugin-react": "^5.0.1",
    "eslint": "^9",
    "eslint-config-next": "15.4.7",
    "jsdom": "^26.1.0",
    "tailwindcss": "^4",
    "typescript": "^5",
    "vitest": "^3.2.4"
  }
}